<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
 
<mapper namespace="com.nexcloud.rdb.mapper.mysql.LogRepository">
	<select id="getLogList" resultType="com.nexcloud.rdb.domain.mysql.Log">
		SELECT
			idx,
			host_ip hostIp,
			container_id containerId,
			stream,
			log,
			time
			regdt
		FROM
			nexclipper_log
	</select>
</mapper>
